          <div class="FRigoo"><h2>Frigoo</h2></div>
        </div>
        <div class="rooms" data-aos="fade-up" data-aos-duration="2000">
          <div class="head">
            <div class="room-head"><h2>rooms</h2></div>
          </div>
          <div class="room-container" id="room-container">
            <!-- <% Object.keys(rooms).forEach(room => { %> -->
            <div class="room-name">
              <div class="name"><%= room %></div>
              <div><a href="/<%= room %>" class="join">Join</a></div>
            </div>
            <!-- <% }) %> -->
          </div>

          <div class="form">
            <form action="/room" method="POST">
              <input
                name="room"
                type="text"
                required
                placeholder="Enter room name . . ."
              />
              <button type="submit">make room</button>
            </form>
          </div>
        </div>
      </div>
    </div>
    <div class="preloader">
      <div class="body">
        <span>
          <span></span>
          <span></span>
          <span></span>
          <span></span>
        </span>
        <div class="base">
          <span></span>
          <div class="face"></div>
        </div>
      </div>
      <div class="longfazers">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
      </div>
      <h1>Loading ...</h1>
    </div>
  </body>
  <script>
    $(window).on("load", function () {
      $(".preloader").fadeOut("slow");
      $(".main-page").css("display", "flex");
